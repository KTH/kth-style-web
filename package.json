{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kth-style-web",
  "version": "3.0.0",
  "description": "Template project for a node express web server",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/kth-style-web"
  },
  "scripts": {
    "vendor": "parcel build  ./public/js/vendor.js  --public-url /style/static",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "cross-env NODE_ENV=production && ./build.sh prod",
    "build-dev": "cross-env NODE_ENV=development && ./build.sh dev",
    "parcel-views": "parcel build ./public/js/app/app.jsx ./public/js/app/serverSideFunctions.js --public-url /style/static",
    "parcel-sass-prod": "parcel build ./public/css/**/*.scss --no-source-maps --public-url /style/static",
    "parcel-sass-dev": "parcel build ./public/css/**/*.scss --public-url /style/static",
    "parcel-watch": "parcel watch ./public/js/app/app.jsx --public-url /style/static && parcel watch ./public/css/*.scss --public-url /style/static",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others \"nodemon app.js\" \"npm run parcel-watch\"",
    "docker:start": "docker build --tag kth-style-web:local . && docker images --filter=reference=kth-style-* && echo \"\\n\\nStarting container - don't forget 'npm run docker:stop' when you are done.\\n\\n\" && docker run --publish 3001:3001 --env-file .env kth-style-web:local",
    "docker:stop": "bash -c 'IDS1=`docker ps --filter ancestor=kth-style-web:local -aq`; if [ -n \"$IDS1\" ]; then docker rm -v --force \"$IDS1\"; fi; IDS2=`docker ps --filter status=exited -aq`; if [ -n \"$IDS2\" ]; then docker rm -v \"$IDS2\"; fi'",
    "docker:clean": "npm run docker:stop && bash -c 'docker rmi $(docker images --quiet kth-style-web:local)'"
  },
  "dependencies": {
    "applicationinsights": "^1.8.7",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.1.0",
    "jquery": "3.5.1",
    "kth-node-access-log": "^0.2.2",
    "kth-node-api-call": "^3.0.12",
    "kth-node-configuration": "^2.0.3",
    "kth-node-express-routing": "^2.0.3",
    "kth-node-i18n": "^1.0.3",
    "kth-node-ldap": "^4.0.0",
    "kth-node-log": "^3.1.3",
    "kth-node-monitor": "^1.1.8",
    "kth-node-passport-cas": "^3.1.12",
    "kth-node-redis": "^3.1.7",
    "kth-node-server": "^3.1.2",
    "kth-node-session": "^1.0.4",
    "kth-node-web-common": "^5.1.4",
    "kth-style": "^6.0.0",
    "locale": "^0.1.0",
    "mobx": "^6.0.1",
    "mobx-react": "^7.0.0",
    "passport": "^0.4.1",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "safe-utils": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.5.2",
    "jest-axe": "^4.0.0",
    "jest-emotion": "^10.0.32",
    "jest-extended": "^0.11.5",
    "merge-stream": "^2.0.0",
    "mocha": "8.1.3",
    "mockery": "^2.1.0",
    "nock": "^13.0.4",
    "node-mocks-http": "^1.9.0",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.27.0",
    "zombie": "6.1.4"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  },
  "engines": {
    "node": "12"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "husky": {
    "hooks": {
      "pre-push": "CI=1 npm run test",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
