{
  "name": "kth-style-web",
  "version": "3.0.0",
  "description": "Template project for a node express web server",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/kth-style-web"
  },
  "scripts": {
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "postinstall": "bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "applicationinsights": "^2.1.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.2",
    "jquery": "3.6.0",
    "kth-node-access-log": "^0.2.4",
    "kth-node-api-call": "^3.0.16",
    "kth-node-configuration": "^2.0.5",
    "kth-node-express-routing": "^2.0.9",
    "kth-node-i18n": "^1.0.10",
    "kth-node-ldap": "^4.0.3",
    "kth-node-log": "^3.1.7",
    "kth-node-monitor": "^1.1.12",
    "kth-node-passport-cas": "^3.1.15",
    "kth-node-redis": "^3.1.30",
    "kth-node-server": "^3.1.5",
    "kth-node-session": "^1.0.6",
    "kth-node-web-common": "^5.1.16",
    "kth-style": "^6.0.21",
    "locale": "^0.1.0",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "passport": "^0.4.1",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "safe-utils": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@kth/eslint-config-kth": "^1.0.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "concurrently": "^6.2.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "jest-emotion": "^10.0.32",
    "jest-extended": "^0.11.5",
    "merge-stream": "^2.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "mocha": "9.0.1",
    "mockery": "^2.1.0",
    "nock": "^13.1.0",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.7",
    "null-loader": "^4.0.1",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "zombie": "6.1.4"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  }
}
