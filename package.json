{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kth-style-web",
  "version": "3.0.0",
  "description": "Template project for a node express web server",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/kth-style-web"
  },
  "engines": {
    "node": "12"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod /style",
    "build-dev": "bash ./build.sh dev /style",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "bash -c 'NODE_ENV=development concurrently -n app,build --kill-others \"nodemon app.js\" \"npm run build-dev\"'",
    "docker": "npm install --development && npm run build && npm prune --production"
  },
  "dependencies": {
    "applicationinsights": "^1.8.7",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "jquery": "3.5.1",
    "kth-node-access-log": "^0.2.2",
    "kth-node-api-call": "^3.0.12",
    "kth-node-configuration": "^2.0.3",
    "kth-node-express-routing": "^2.0.3",
    "kth-node-i18n": "^1.0.3",
    "kth-node-ldap": "^4.0.0",
    "kth-node-log": "^3.1.3",
    "kth-node-monitor": "^1.1.8",
    "kth-node-passport-cas": "^3.1.12",
    "kth-node-redis": "^3.1.7",
    "kth-node-server": "^3.1.2",
    "kth-node-session": "^1.0.4",
    "kth-node-web-common": "^5.1.4",
    "kth-style": "^6.0.1",
    "locale": "^0.1.0",
    "mobx": "^6.0.1",
    "mobx-react": "^7.0.0",
    "passport": "^0.4.1",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "safe-utils": "^0.2.1"
  },
  "devDependencies": {
    "@kth/eslint-config-kth": "^1.0.2",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "jest-axe": "^4.0.0",
    "jest-emotion": "^10.0.32",
    "jest-extended": "^0.11.5",
    "merge-stream": "^2.0.0",
    "mocha": "8.2.0",
    "mockery": "^2.1.0",
    "nock": "^13.0.4",
    "node-mocks-http": "^1.9.0",
    "nodemon": "^2.0.6",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.27.0",
    "zombie": "6.1.4"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
